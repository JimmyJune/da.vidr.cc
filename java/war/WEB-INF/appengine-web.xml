<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://appengine.google.com/ns/1.0 http://googleappengine.googlecode.com/svn/trunk/java/docs/appengine-web.xsd">
    <application>dar-java</application>
    <version>1</version>
    
    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
    </system-properties>
    
    <static-files>
        <exclude path="/**.php" />
    </static-files>
    <resource-files>
        <include path="/**.php" />
    </resource-files>
</appengine-web-app>
